<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" prefHeight="330.0" prefWidth="342.0" vgap="10"
  xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="sample.Controller">
  <columnConstraints>
    <ColumnConstraints/>
    <ColumnConstraints/>
    <ColumnConstraints/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints/>
    <RowConstraints/>
    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    <RowConstraints maxHeight="221.33333333333334" minHeight="221.33333333333334"
      prefHeight="221.33333333333334"/>
    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    <RowConstraints/>
  </rowConstraints>
  <children>
    <TabPane prefHeight="313.0" prefWidth="300.0" tabClosingPolicy="UNAVAILABLE"
      GridPane.rowIndex="3">
      <tabs>
        <Tab fx:id="ProduceLineTab" text="Product Line">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <AnchorPane fx:id="ProductLineAP" prefHeight="193.0" prefWidth="300.0">
                  <children>
                    <GridPane layoutX="14.0" layoutY="14.0" prefHeight="113.0" prefWidth="285.0">
                      <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="147.0" minWidth="10.0"
                          prefWidth="135.6666259765625"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.33338928222656"
                          minWidth="10.0" prefWidth="150.3333740234375"/>
                      </columnConstraints>
                      <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                      </rowConstraints>
                      <children>
                        <Label fx:id="ProductNameLabel" prefHeight="27.0" prefWidth="156.0"
                          text="Product Name" textFill="#35bff0"/>
                        <Label fx:id="ManufacturerLabel" prefHeight="27.0" prefWidth="156.0"
                          text="Manufacturer" textFill="#2ab1f9" GridPane.rowIndex="1"/>
                        <Label fx:id="ItemTypeGrid" prefHeight="22.0" prefWidth="101.0"
                          text="Item Type" textFill="#32b1f0" GridPane.rowIndex="2"/>
                        <TextField fx:id="ProductNameTextField" prefHeight="25.0" prefWidth="83.0"
                          GridPane.columnIndex="1"/>
                        <TextField fx:id="ManufacturerTextField" GridPane.columnIndex="1"
                          GridPane.rowIndex="1"/>
                        <ChoiceBox fx:id="ChoiceBoxItemType" prefWidth="150.0"
                          GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                      </children>
                    </GridPane>
                    <Button fx:id="AddProductButton" layoutX="210.0" layoutY="140.0"
                      mnemonicParsing="false" onAction="#addProductButtonClick" text="Add Product"
                      textFill="#3290c6"/>
                  </children>
                </AnchorPane>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="ProduceTab" text="Produce">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <AnchorPane fx:id="ProduceAP" layoutX="-6.0" layoutY="-1.0" prefHeight="193.0"
                  prefWidth="306.0">
                  <children>
                    <Label fx:id="ChooseProductLabel" layoutX="14.0" layoutY="14.0"
                      text="Choose Product"/>
                    <ListView fx:id="ChooseProductListView" layoutX="106.0" layoutY="9.0"
                      prefHeight="69.0" prefWidth="172.0"/>
                    <Label fx:id="ChooseQuantityLabel" layoutX="12.0" layoutY="114.0"
                      text="Choose Quantity"/>
                    <ComboBox fx:id="chooseQuanityComboBox" layoutX="128.0" layoutY="110.0"
                      prefWidth="150.0"/>
                    <Button fx:id="RecordProductionButton" layoutX="166.0" layoutY="154.0"
                      mnemonicParsing="false" onAction="#recordProButton" text="Record Production"/>
                  </children>
                </AnchorPane>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="ProductionLogTab" text="Production Log">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <AnchorPane fx:id="ProductionLogAP" prefHeight="192.0" prefWidth="300.0">
                  <children>
                    <TextArea fx:id="TextArea" layoutX="50.0" layoutY="16.0" prefHeight="142.0"
                      prefWidth="200.0"/>
                  </children>
                </AnchorPane>
              </children>
            </AnchorPane>
          </content>
        </Tab>
      </tabs>
    </TabPane>
  </children>
</GridPane>
